diagram admin {
  // simple diagram.
  A0 -> A1 -> A2;
  A0 -> A3 -> A4;

  // circular references.
  B0 -> B1 -> B2 -> B0;
        B1 -> B3;

  // double references (with circular ref.)
  C0 -> C1 -> C2 -> C0;
  C0       -> C2;

  // triple references
  D0 -> D3;
  D1 -> D3;
  D2 -> D3;

  // multiple circular references.
  E0 -> E1 -> E0;
  E0 -> E2 -> E0;

  // self reference.
  F0 -> F1 -> F1;

  // single node.
  Z0;
}
